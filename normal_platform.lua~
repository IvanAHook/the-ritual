require('Platform')

normal_platform = Platform:create()

function normal_platform:new()
    setmetatable(normal_platform,self)
    self.__index = self
    return normal_platform
end

function normal_platform:spawn(world,x,y,width,height,body_type)
    Platform:spawn(world,x,y,width,height,body_type)
end

function normal_platform:load()
    self.red = 30
    self.green = 30
    self.blue = 30
end

function normal_platform:draw()
    love.graphics.setColor(self.red, self.green, self.blue )
    Platform:draw()
end
